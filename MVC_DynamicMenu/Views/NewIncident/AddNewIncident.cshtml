
@model NewIncidentLog
@using Newtonsoft.Json;
@using Microsoft.AspNetCore.Http;
@{
    ViewData["Title"] = "AddNewClient";
    List<Patient> cli = JsonConvert.DeserializeObject<List<Patient>>(Context.Session.GetString("incident"));
    string loggeduser = Context.Session.GetString("loggedname");
}
<head>
    <link href="~/css/Addfile.css" rel="stylesheet" />
</head>
<div class="container">
    <header class="header">
        <h1 id="title" class="text-center">Form :Add A New Incident Log </h1>
        <p id="description" class="description text-center">

        </p>
    </header>
    <form asp-action="AddNewIncident" id="survey-form">
        <div class="form-group">
            <label id="name-label" asp-for="Client">Client</label>
            <select class="form-control" asp-for="Client" placeholder="Pick a Name">
                @foreach (var item in cli)
                {
    <option value="@item.Full_Name">@item.Full_Name</option>
}

            </select>
        </div>


        @*Date Start*@
        <label asp-for="EntryDtae">Entry Date</label>
        <input type="date" asp-for="EntryDtae" id="dateofbirth">

        @*Hierarchy Start*@
        <div class="form-group">
            <p>Hierarchy</p>
            <select id="dropdown" asp-for="Heirarchy" class="form-control" >
                @*<option disabled selected value>Select current role</option>*@
                <option value="ACT_Springfield_Evergreen">ACT Springfield Evergreen</option>
                <option value="NWS_Demo_Region">NWS Demo Region</option>
                <option value="NSW_Demo_Region_A_Unit_1_3">NSW Demo Region A Unit 1 3</option>
                <option value="NSW_Demo_Region_2_Unit_2_1_A">NSW Demo Region 2 Unit 2 1 A</option>
                <option value="QLD_Region_4_Bridging_Famillies">QLD Region 4 Bridging Famillies</option>
                <option value="QLD_Region_Younger_parents_program">QLD Region Younger parents program</option>
                <option value="NWS_Eiver_Dischange_review">NWS Eiver Dischange review</option>
            </select>
        </div>

        @*Type of Incident*@
        <div class="form-group">

            <p>Type of Incident</p>
            <h6>Incident Type</h6>
            <label>
                <input asp-for="IncidentType"
                       value="Accident"
                       type="radio"
                       class="input-radio"
                       checked />Accident
            </label>
            <label>
                <input asp-for="IncidentType"
                       value="Medication"
                       type="radio"
                       class="input-radio" />Medication
            </label>

            <label>
                <input asp-for="IncidentType"
                       value="Benaviour"
                       type="radio"
                       class="input-radio" />Benaviour
            </label>

        </div>

        @*General Information*@
        <button type="button" class="accordion">General Information</button>
        <div class="panel">
            <p>General Information</p>
            <br />
            <h6>Incident Date</h6>

            <label asp-for="IncidentDate">Entry Date</label>
            <input type="date" asp-for="IncidentDate" id="dateofbirth">

            <div class="form-group">
                <label id="name-label" asp-for="IncidentTime">Time</label>
                <input type="time"
                       asp-for="IncidentTime"
                       id="name"
                       class="form-control"
                       required />
            </div>

            <div class="form-group">


                <h6>Location</h6>
                <textarea id="comments"
                          class="input-textarea"
                          asp-for="IncidentLocation"></textarea>
            </div>

            <div class="form-group">


                <h6>Emergency Assistance</h6>
                <label>
                    <input asp-for="EmergencyAssistance"
                           value="Ambulance"
                           type="radio"
                           class="input-radio"
                           checked />Ambulance
                </label>
                <label>
                    <input asp-for="EmergencyAssistance"
                           value="Fire Brigade"
                           type="radio"
                           class="input-radio" />Fire Brigade
                </label>

                <label>
                    <input asp-for="EmergencyAssistance"
                           value="Police"
                           type="radio"
                           class="input-radio" />Police
                </label>
                <label>
                    <input asp-for="EmergencyAssistance"
                           value="Security Guards"
                           type="radio"
                           class="input-radio" />Security Guards
                </label>

            </div>

            <div class="form-group">


                <h6>Police Involvement</h6>
