                        <br />

                        <h6>AM Staff</h6>
                        <br />
                        <select id="dropdown" asp-for="AM_staff" class="form-control" required>
                            @*<option disabled selected value>Gendere</option>*@
                            <option value="Akwele Acquah">Akwele Acquah</option>
                            <option value="Takudzwa Matsatsa">Takudzwa Matsatsa</option>
                            <option value="Anju George">Anju George</option>
                            <option value="Janet Kirubi">Janet Kirubi</option>
                            <option value="Maxwell EDIZIE">Maxwell EDIZIE</option>
                            <option value="Faida Mushegero">Faida Mushegero</option>
                            <option value="Vincent Masuku">Vincent Masuku</option>
                            <option value="Letty Hearburns">Letty Hearburns</option>
                            <option value="Jasmine Ali">Jasmine Ali</option>
                            <option value="Prince Markedenge">Prince Markedenge</option>
                            <option value="Linda sutton">Linda sutton</option>
                            <option value="Joy Mathew">Joy Mathew</option>
                            <option value="Prince Test">Prince Test</option>
                            <option value="Micheal Masatsa">Micheal Masatsa</option>
                            <option value="Lisa FEHOKO">Lisa FEHOKO</option>
                            <option value="Tom Ondicho">Tom Ondicho</option>

                        </select>
                        <br />

                        <label id="name-label" asp-for="Duration_time">Duration Hours</label>
                        <br />
                        <input data-placeholder="Duration Hours" type="number"
                               asp-for="Duration_time"
                               id="name"
                               class="form-control"
                               required />
                        <br />

                        <h6>Night Shift</h6>
                        <br />
                        <select data-placeholder="--Select--" id="dropdown" asp-for="Night_shift" class="form-control" required>
                            @*<option disabled selected value>Gendere</option>*@
                            <option value="student">Active Night </option>
                            <option value="student">Sleep Over</option>


                        </select>
                        <br />

                        <h6>Program</h6>
                        <br />
                        <select data-placeholder="--Program--" id="dropdown" asp-for="Program" class="form-control" required>
                            @*<option disabled selected value>Gendere</option>*@
                            <option value="NDIS">Select Contact Type</option>
                            <option value="Respite">Phone</option>
                            <option value="SIL">Face to Face</option>
                            <option value="Short Term Accommodation">Did not Attend</option>
                            <option value="student">Email</option>


                        </select>
                        <br />

                        <h6>Sticky Note?</h6>
                        <label>
                            <input asp-for="Stick_note"
                                   value="Yes"
                                   type="radio"
                                   class="input-radio"
                                   checked />Yes
                        </label>
                        <br />
                        <label>
                            <input asp-for="Stick_note"
                                   value="No"
                                   type="radio"
                                   class="input-radio" />No
                        </label>
                        <br />

                        <h6>PM Staff</h6>
                        <br />
                        <select id="dropdown" asp-for="PM_Staff" class="form-control" required>
                            @*<option disabled selected value>Gendere</option>*@
                            <option value="Akwele Acquah">Akwele Acquah</option>
                            <option value="Takudzwa Matsatsa">Takudzwa Matsatsa</option>
                            <option value="Anju George">Anju George</option>
                            <option value="Janet Kirubi">Janet Kirubi</option>
                            <option value="Maxwell EDIZIE">Maxwell EDIZIE</option>
                            <option value="Faida Mushegero">Faida Mushegero</option>
                            <option value="Vincent Masuku">Vincent Masuku</option>
                            <option value="Letty Hearburns">Letty Hearburns</option>
                            <option value="Jasmine Ali">Jasmine Ali</option>
                            <option value="Prince Markedenge">Prince Markedenge</option>
                            <option value="Linda sutton">Linda sutton</option>
                            <option value="Joy Mathew">Joy Mathew</option>
                            <option value="Prince Test">Prince Test</option>
                            <option value="Micheal Masatsa">Micheal Masatsa</option>
                            <option value="Lisa FEHOKO">Lisa FEHOKO</option>
                            <option value="Tom Ondicho">Tom Ondicho</option>


                        </select>
                        <br />


                        <label id="name-label" asp-for="Minutes">Minutes</label>
                        <br />
                        <input data-placeholder="Duration Minutes" type="number"
                               asp-for="Minutes"
                               id="name"
                               class="form-control"
                               required />
                        <br />

                        <h6>Contact Type</h6>
                        <br />
                        <select data-placeholder="--Select Cotact Type--" id="dropdown" asp-for="Contact_type" class="form-control" required>
                            @*<option disabled selected value>Gendere</option>*@
                            <option value="Phone">Phone</option>
                            <option value="Face to face">Face to face</option>
                            <option value="Did not attend">Did not attend</option>
                            <option value="Email">Email</option>
                            <option value="Group Conference">Group Conference</option>
                            <option value="Mail">Mail</option>
                            <option value="Internet/Online forms">Internet/Online forms</option>
                            <option value="Skype">Skype</option>
                            <option value="Text">Text</option>
                            <option value="Zoom Meeting">Zoom Meeting</option>


                        </select>
                        <br />
                        <h6>With</h6>
                        <br />
                        <select data-placeholder="--Contacted?--" id="dropdown" asp-for="with" class="form-control" required>
                            @*<option disabled selected value>Gendere</option>*@
                            <option value="Participant">Participant</option>
                            <option value="Family member">Family member</option>
                            <option value="3rd Party">3rd Party</option>
                            <option value="Public Guardian">Public Guardian</option>
                            <option value="Partners">Partners</option>
                            <option value="Support Coordinator">Support Coordinator</option>
                        </select>
                        <br />

                        <h6>Referral To</h6>
                        <br />
                        <select data-placeholder="--Referre to?--" id="dropdown" asp-for="Referral_to" class="form-control" required>
                            @*<option disabled selected value>Gendere</option>*@
                            <option value="Hospital">Hospital</option>
                            <option value="Police">Police</option>
                        </select>
                        <br />
                        <label id="name-label" asp-for="Brokerage">Brokerage</label>
                        <br />
                        <input data-placeholder="$" type="number"
                               asp-for="Brokerage"
                               id="name"
                               class="form-control"
                               required />
                        <br />

                    </div>

                </div>

                <br />

                @*Recent Case Notes
                *@

                <button type="button" class="accordion">Recent Case Notes</button>
                <div class="panel">
                    <div class="form-group">


                        <h6>
                            Recent Case Notes
                        </h6>
                        <textarea id="comments"
                                  class="input-textarea"
                                  asp-for="Recent_case_note">

                         </textarea>

                    </div>
                </div>

                <br />

            </div>
        </div>
        @*Daily Checks*@


        <button type="button" class="accordion">
            Daily Checks
        </button>
