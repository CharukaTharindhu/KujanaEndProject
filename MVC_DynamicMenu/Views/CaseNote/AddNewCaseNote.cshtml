@model NewCaseNote
@using Newtonsoft.Json;
@using Microsoft.AspNetCore.Http;
@{
    ViewData["Title"] = "AddNewClient";
    List<Patient> cli = JsonConvert.DeserializeObject<List<Patient>>(Context.Session.GetString("note"));
}
<head>
    <link href="~/css/Addfile.css" rel="stylesheet" />
</head>
<div class="container">
    <header class="header">
        <h1 id="title" class="text-center">New Note</h1>
        <p id="description" class="description text-center">

        </p>
    </header>

    <form asp-action="AddNewCaseNote" id="survey-form">
        <div class="row">
            <div class="col-md-6">


                @*Goals Achieved
                *@


                <button type="button" class="accordion">Goals Achieved</button>
                <div class="panel">
                    <div class="form-group">


                        <h6>
                            Goals Achieved
                        </h6>
                        <textarea id="comments"
                                  class="input-textarea"
                                  asp-for="Goals_achieved">

                         </textarea>
                    </div>
                </div>

                <br />
                @*Attachments
                *@


                <button type="button" class="accordion">Attachments</button>
                <div class="panel">
                    <div class="form-group">


                        <h6>
                            Attachments
                        </h6>
                        <textarea id="comments"
                                  class="input-textarea"
                                  asp-for="Attachement">

                         </textarea>

                    </div>

                </div>

                <br />
                <br />

            </div>
            <br />
            <br />
            <div class="col-md-6">

